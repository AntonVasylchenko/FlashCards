generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id           String  @id @default(auto()) @map("_id") @db.ObjectId
  telegramId   Int     @unique
  firstName    String?
  lastName     String?
  userName     String?
  languageCode String?
}
